#include<stdio.h>
int main(){

    int n;
    scanf("%d" , &n);
    int a[n];
    for( int i=0 ; i<n ; i++){
        scanf("%d",&a[i]);
    }
    int min=a[0];
    int max = a[0];
    int count1=0;
    int count2=0;
    for( int i=0 ; i<n ; i++){
        if(max<a[i]){
            max = a[i];
            count1++;
        }
        if(min>a[i]){
            min=a[i];
            count2++;
        }
    }
    printf("%d " , count1);
    printf("%d" , count2);



    return 0;
}
